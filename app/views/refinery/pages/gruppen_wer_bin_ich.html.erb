<!-- PageParts -->
<div class="container">
  <section id='placeholder'>
    <%=raw @page.content_for(:placeholder) %>
  </section>
  <section id='headline'>
    <%=raw @page.content_for(:headline) %>
  </section>
</div>

<div class="inner-container">
  <div>
    <section id='img_box_1' class="small">
      <%=raw @page.content_for(:img_box_1) %>
    </section>
  </div>
  <section class="text_box">
    <div class="gruppen_title">
      <%=raw @page.content_for(:title) %>
    </div>
    <div class="gruppen-subtitle">
      <%=raw @page.content_for(:subtitle) %>
    </div>
    <div>
      <%=raw @page.content_for(:text_box_1) %>
    </div>
  </section> <!-- text_box -->
  <p class="clearfix"></p>
</div> <!-- inner-container -->
